{
  "schemaVersion": 1,
  "id": "dpu",
  "version": "${version}",
  "name": "DatapackUtils",
  "description": "Fabric mod which adds features to the vanilla Resource and Data pack system",
  "authors": [
    "Feintha"
  ],
  "contact": {
    "repo": "https://github.com/avetharun/FabricDatapackUtils"
  },
  "license": "GPL-3.0",
  "icon": "dpu.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.feintha.dpu.client.DatapackUtilsClient"
    ],
    "main": [
      "com.feintha.dpu.DatapackUtils"
    ]
  },
  "accessWidener" : "dpu.accesswidener",
  "mixins": [
    "dpu.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
